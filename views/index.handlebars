<h1>Eat-Da-Burgers!</h1>

<h2>All Available Burgers</h2>

<ul>
    {{#each burgers}}
        <li>{{id}}</li>
        <li>{{burger_name}}</li>
        <li>{{devoured}}</li>
    {{/each}}
</ul>

<br>
<br>

<h2>Add a Burger</h2>
<form class="create-form">

    <div class="form-group">
        <label for="burger">Burger Name:</label>
        <input type="text" id="new-burger" name="burgerName">
    </div>

    <button type="submit">Add Burger</button>
</form>